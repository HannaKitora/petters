<h1>Event</h1>

<%= form_with url: admin_events_path, model: @event do |f| %>
  <h4>image</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>Title</h4>
  <%= f.text_field :name %>

  <h4>Price</h4>
  <%= f.text_field :price %>

  <h4>Detail</h4>
  <%= f.text_area :detail %>

  <%= f.submit 'POST' %>
<% end %>